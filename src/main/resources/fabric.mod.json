{
	"schemaVersion": 1,
	"id": "indicativepitch",
	"version": "${version}",
	"name": "Indicative Pitch",
	"description": "Makes block breaking sounds higher pitched as the block gets closer to being broken.",
	"authors": [
		"Startraveler"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/indicativepitch/icon.png",
	"environment": "*",
	"entrypoints": {
		"client": [
			"com.startraveler.indicativepitch.IndicativePitchClient"
		]
	},
	"mixins": [
		{
			"config": "indicativepitch.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.13",
		"minecraft": ">=1.21.2 <=1.22",
		"java": ">=21"
	}
}